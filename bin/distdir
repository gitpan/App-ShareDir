#!/usr/bin/env perl
# PODNAME: distdir
# ABSTRACT: Executing File::ShareDir dist_dir with the arguments

$|=1;

use strict;
use warnings;
use File::ShareDir ':ALL';

print dist_dir(@ARGV)."\n";

__END__

=pod

